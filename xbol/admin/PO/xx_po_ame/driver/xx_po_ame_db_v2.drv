begin patch xx_po_ame_db_v2

  begin patch_info 
  release  2
  server   DB
  language US
  end patch_info 

  copy xbol /admin/PO/xx_po_ame/driver xx_po_ame_db_v2.drv

  copy xbol /admin/PO/xx_po_ame/driver buildpatch_db_v2.sh

  copy xbol /admin/PO/xx_po_ame/sql XX_PO_AME_UTILB_PK.sql

  begin connection apps

    sql xbol $XBOL_TOP/admin/PO/xx_po_ame/sql XX_PO_AME_UTILB_PK.sql

  end connection apps

end patch xx_po_ame_db_v2
